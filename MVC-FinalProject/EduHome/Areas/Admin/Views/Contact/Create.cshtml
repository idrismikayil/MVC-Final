@model Contact

@{
    ViewData["Title"] = "Create";
}

      <div class="main-panel">        
        <div class="content-wrapper">
          <div class="row">
            <div class="col-md-6 grid-margin stretch-card">
              <div class="card">
                <div class="card-body">
                  <h4 class="card-title">Create Contact</h4>
                  
                  <form class="forms-sample" method="post"  asp-action="Create" enctype="multipart/form-data">
                           
                            <div class="form-group">
                                <label for="exampleInputUsername1">Logo 1</label>
                                <input accept="image/*" asp-for="ImageFile1" class="form-control" placeholder="Image">
                                <span asp-validation-for="ImageFile1" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label for="exampleInputUsername1">Logo 2</label>
                                <input accept="image/*" asp-for="ImageFile2" class="form-control" placeholder="Image">
                                <span asp-validation-for="ImageFile2" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label for="exampleInputUsername1">Logo 3</label>
                                <input accept="image/*" asp-for="ImageFile3" class="form-control" placeholder="Image">
                                <span asp-validation-for="ImageFile3" class="text-danger"></span>
                            </div>
                          
                             <div class="form-group">
                                <label for="exampleInputUsername1">Addres 1</label>
                                <input asp-for="AddressOne" class="form-control" placeholder="">
                                <span asp-validation-for="AddressOne" class="text-danger"></span>
                            </div>
                             <div class="form-group">
                                <label for="exampleInputUsername1">Addres 2</label>
                                <input asp-for="AddressTwo" class="form-control" placeholder="">
                                <span asp-validation-for="AddressTwo" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label for="exampleInputUsername1">Number </label>
                                <input asp-for="Number" class="form-control" placeholder="">
                                <span asp-validation-for="Number" class="text-danger"></span>
                            </div>

                            <button type="submit" class="btn btn-primary me-2"> Submit</button>
                            <a class="btn btn-light" asp-action="Index">Cancel</a>
                        </form>
                </div>
              </div>
             </div>
           </div>
          </div>
         </div>