@model  List<Contact>
@{
    ViewData["Title"] = "Index";
}
      <div class="main-panel">
        <div class="content-wrapper">
          <div class="row">
            <div class="col-lg-12 stretch-card">
              <div class="card">
                <div class="card-body">
                  <h4 class="card-title">Contact</h4>
                  
                  <div class="table-responsive pt-3">
                    <table class="table table-bordered">
                      <thead>
                        <tr>
                          <th>
                            Id
                          </th>
                          <th>
                             Address 1
                          </th>
                          <th>
                              Address 2
                          </th>
                          <th>
                              Number 
                          </th>
                          
                           <th>
                            Actions
                          </th>
                        </tr>
                      </thead>
                      <tbody>

                       @foreach (var c in Model)
                      {
                           <tr class="table-primary">
                          <td>
                            @c.Id
                          </td>
                          <td>
                            @c.AddressOne
                          </td>
                          <td>
                            @c.AddressTwo
                          </td>
                          <td>
                            @c.Number
                          </td>
                          
                         
                          <td>
                          <a class="btn btn-primary" asp-action="Detail" asp-route-id="@c.Id">Detail</a>
                          <a class="btn btn-warning" asp-action="Update" asp-route-id="@c.Id">Update</a>
                          <a class="btn btn-danger" asp-action="Delete" asp-route-id="@c.Id">Delete</a>
                          </td>
                        </tr>
                      }
                    
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>



